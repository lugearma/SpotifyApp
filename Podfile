  # Comment this line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for SpotifyApp
    pod 'Alamofire', '~> 3.4'
    pod 'SDWebImage', '~>3.7'

  target 'SpotifyAppTests' do
